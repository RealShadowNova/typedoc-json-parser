{
  "name": "typedoc-json-parser",
  "version": "3.2.0",
  "description": "A package to parse TypeDoc JSON output",
  "author": "Hezekiah Hendry <hezekiah.hendry@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "bin": "./dist/bin/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist docs node_modules/.cache",
    "docs": "yarn build && node dist/bin/index.js --json docs/api.json",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf dist && tsc -b src && gen-esm-wrapper dist/index.js dist/index.mjs && node scripts/versionInjector.mjs",
    "update": "yarn upgrade-interactive",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "postinstall": "husky install",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@favware/colorette-spinner": "^1.0.1",
    "colorette": "^2.0.19",
    "commander": "^9.4.0",
    "js-yaml": "^4.1.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@favware/cliff-jumper": "^1.8.7",
    "@favware/npm-deprecate": "^1.0.5",
    "@sapphire/eslint-config": "^4.3.8",
    "@sapphire/ts-config": "^3.3.4",
    "@types/jest": "^29.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.38",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gen-esm-wrapper": "^1.1.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "lint-staged": "^12.5.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealShadowNova/typedoc-json-parser.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/RealShadowNova/typedoc-json-parser/issues"
  },
  "homepage": "https://github.com/RealShadowNova/typedoc-json-parser",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  }
}
