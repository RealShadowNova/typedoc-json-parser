{
  "name": "typedoc-json-parser",
  "version": "1.0.0",
  "description": "A package to parse TypeDoc JSON output",
  "author": "Hezekiah Hendry <real.shadow.nova@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "bin": "./dist/bin/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist docs node_modules/.cache",
    "docs": "typedoc",
    "debug": "node dist/bin/index.js -v --json docs/api.json",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write src/**/*.ts",
    "build": "tsc -b src",
    "update": "yarn upgrade-interactive",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "postinstall": "husky install",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "colorette": "^2.0.16",
    "commander": "^9.3.0",
    "js-yaml": "^4.1.0",
    "ora-classic": "^5.4.2",
    "typedoc": "^0.22.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@favware/cliff-jumper": "^1.8.0",
    "@favware/npm-deprecate": "^1.0.4",
    "@sapphire/eslint-config": "^4.3.5",
    "@sapphire/ts-config": "^3.3.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.38",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealShadowNova/typedoc-json-parser.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/RealShadowNova/typedoc-json-parser/issues"
  },
  "homepage": "https://github.com/RealShadowNova/typedoc-json-parser",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  }
}
